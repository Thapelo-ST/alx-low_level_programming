#ifndef MAIN_H
#define MAIN_H
#include <stdio.h>

/**
 * _putchar - passes a charecter into putchar
 * print_alphabet_x10 - prints the alphabet in lowercase 10 times
 * print_alphabet - prints alphabets and end in new line
 * Return: void
 */

/*task 0*/
int _putchar(int p)
{
	putchar(p);
}
/*task 1*/
void print_alphabet(void)
{
	chr lett = 'a';

	while (lett < 'z')
	{
		_putchar(lett);
		lett++;
	}
}
/*task 2*/
void print_alphabet_x10(void)
{
	char lett = 'a';
	int count = 0;

	while (count < 10)
	{
		while (lett < 'z')
		{
			_putchar(lett);
			lett++;
		}
		_putchar('\n');
		count++;
	}
}
#endif /* MAIN_H */
